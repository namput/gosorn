{
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/blog/:match*",
      "destination": "/blog/:match*.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}